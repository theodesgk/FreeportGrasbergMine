<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="modules" type="Microsoft.Practices.Prism.Modularity.ModulesConfigurationSection, Microsoft.Practices.Prism.Composition" />
    <section name="formsRepositoryProviderSection" type="Modular.MineView.Implementation.Presentation.FormsLauncher.Configuration.FormsRepositoryProviderSection, Modular.MineView.Client.Implementation" />
    <section name="mineGraphics" type="Modular.MineView.Implementation.DataObjects.Configuration.MineGraphics.MineGraphicsConfigSection, Modular.MineView.Client.Implementation" />
    <section name="commonPreferences" type="Modular.MineView.Implementation.DataObjects.Configuration.Preferences.CommonPreferencesSection, Modular.MineView.Client.Implementation" />
    <section name="haulRoutePreferences" type="Modular.MineView.Implementation.DataObjects.Configuration.Preferences.HaulRoutePreferencesSection, Modular.MineView.Client.Implementation" />
    <section name="mineGraphicsPreferences" type="Modular.MineView.Implementation.DataObjects.Configuration.Preferences.MineGraphicsPreferencesSection, Modular.MineView.Client.Implementation" />
    <section name="implementationFactory" type="Modular.Instantiation.ImplementationFactorySection, Modular.Foundation.Common" />
    <section name="dbioService" type="Modular.Dbio.DataObjects.Configuration.DbioConfigurationSection, Modular.Dbio.Common" />
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <modules>
    <module assemblyFile="Modular.MineView.Client.Implementation.dll" moduleType="Modular.Shell.Module.OnlineHelp, Modular.Shell.Client.Modules" moduleName="OnlineHelp" startupLoaded="true" />
    <module assemblyFile="Modular.MineView.Client.Implementation.dll" moduleType="Modular.MineView.Implementation.MineViewModule, Modular.MineView.Client.Implementation" moduleName="MineView" startupLoaded="true" />
    <module assemblyFile="Modular.MineView.Client.DiagnosticsModule.dll" moduleType="Modular.MineView.Implementation.DiagnosticsModule, Modular.MineView.Client.DiagnosticsModule" moduleName="DiagnosticsModule" startupLoaded="true" />
    <!--<module assemblyFile="Modular.MineView.Client.TaskManagementHostModule.dll" moduleType="Modular.MineView.Implementation.TaskManagementHostModule, Modular.MineView.Client.TaskManagementHostModule" moduleName="TaskManagementHostModule" startupLoaded="true" />-->
  </modules>
  <dbioService configSource="DbioServiceClient.config" />
  <connectionStrings>
    <add name="DiscoveryService" connectionString="net.tcp://localhost:9999/DiscoveryService" />
  </connectionStrings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" autoReload="true">
    <variable name="LogDir" value="AppLog" />
    <variable name="LogFileName" value="MineView" />
    <!--Maximun number of archive files that should be kept. If it is less or equal to 0, the old files aren't deleted -->
    <variable name="MaxArchiveFile" value="0" />
    <include file=".\nLog.config" />
    <rules>
      <!-- logger name format is Category'Component, we can add multiple rules for the same file as below -->
      <!-- <logger name="*FieldCommunication" minlevel="Trace" writeTo="logfile" /> -->
      <!-- <logger name="BusinessLogic*" minlevel="Trace" writeTo="logfile" /> -->
      <logger name="*" minlevel="Info" writeTo="logfile" />
    </rules>
  </nlog>
  <appSettings>
    <add key="ScriptRepository" value="XML" />
    <add key="ScriptRepositoryFile" value="FormsCatalog.xml" />
    <add key="LoggingCategorySchema" value="CombinedCategory" />
    <add key="LicenseFilePath" value="MMS Software License Terms and Conditions.rtf" />
    <!-- Sampling time for MineView statistics in milliseconds -->
    <add key="StatisticsSamplingTime" value="60000" />
    <!-- Wait for the response before reconnect in milliseconds -->
    <add key="WaitTimeBeforeReconnect" value="60000" />
    <!-- Wait for the response default timeout in milliseconds -->
    <add key="DefaultWaitingTimeout" value="5000" />
    <!-- Default number of attempts before reconnect -->
    <add key="DefaultWaitAttemptsNumber" value="5" />
    <!-- Field for Descending sort of statisctics (HitCount, ElapsedTime, AverageSize, Repetition) -->
    <add key="StatisticsDescendingSortField" value="Repetition" />
    <add key="HelpFileHasIndexPage" value="false" />
    <!-- Automatically saves layout on exit and loads on startup -->
    <add key="AutoLayout" value="true" />
    <!--Main window transparency needs to be disabled to host addin components (Task Management)-->
    <add key="DisableMainWindowTransparency" value="true" />
  </appSettings>
  <system.diagnostics>
    <switches>
      <!--Tracing Level Value (0 - Off, 1 - Error, 2 - Warning, 3 - Info, 4 - Verbose) -->
      <add name="TraceInfoSwitch" value="3" />
      <!--Thread Tracing Info Value (0 - Off, 1 - On) -->
      <add name="ThreadInfoSwitch" value="0" />
      <!--Run Time Profiler (0 - Off, 1 - On) -->
      <add name="RunTimeProfilerSwitch" value="0" />
      <!--Record requests to FormsServer (0 - Off, 1 - On) -->
      <add name="RecordFormsServerRequests" value="0" />
      <!--Record request statistics to FormsServer (0 - Off, 1 - On) -->
      <add name="RecordRequestStatistics" value="0" />
    </switches>
  </system.diagnostics>
  <implementationFactory>
    <item name="PerformanceMonitor" type="Modular.MineView.Implementation.Services.PerformanceMonitor, Modular.MineView.Client.Implementation" />
  </implementationFactory>
  <formsRepositoryProviderSection defaultProvider="XmlFomsLauncher">
    <formsRepositoryProviders>
      <add name="XmlFomsLauncher" type="Modular.MineView.Implementation.Presentation.FormsLauncher.Providers.XMLFormsInfoLoaderProvider, Modular.MineView.Client.Implementation" catalogFileName="FormsCatalog.xml" />
    </formsRepositoryProviders>
  </formsRepositoryProviderSection>
  <commonPreferences delayColor="Gold" downColor="Tomato" loadingColor="White" readyColor="LightGreen" shiftChangeColor="LightBeige" standbyColor="LightSkyBlue" currentLocale="en" refreshUtilitiesInterval="2">
    <materialColors>
      <clear />
    </materialColors>
  </commonPreferences>
  <haulRoutePreferences showGraphicalIcons="true" showTextLabels="false" showLoadingProgressBar="false" isShovelCentric="true" isDumpCentric="false" isHaulageOnTop="true" showRegionInfoByTrucks="true" showRegionInfoByTons="false" hasAnimatedRacingPanel="false" background="White" fontScale="100" showUnusedDumps="true" useShovelLockOverride="false" shovelLockOverride="DarkOrange" shovelLockTextOverride="SaddleBrown" />
  <mineGraphicsPreferences roadWidth="10" truckLength="6" majorGridLines="250" minorGridLines="50" labelHeight="6" showGridLabels="True" showCompass="True" zoomIncrement="25" panScale="5" background="#ff808080" minX="0" minY="0" minZ="0" maxX="0" maxY="0" maxZ="0" north="0" mineUnit="Miles" mineUnitAbbreviation="Mi" mineSmallUnit="Feet" mineSmallUnitAbbreviation="Ft" mineUnitFactor="5280.0" useGpsPositioning="false" useAnimation="true">
  </mineGraphicsPreferences>
  <runtime>
    <generatePublisherEvidence enabled="false" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Interop.Security.AzRoles" publicKeyToken="31BF3856AD364E35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.2.0.0" newVersion="1.2.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Practices.ServiceLocation" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.2.0.0" newVersion="1.2.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.OData.Client" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-7.5.4.30215" newVersion="7.5.4.30215" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.OData.Edm" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.9.0.0" newVersion="6.9.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Windows.Interactivity" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.5.0.0" newVersion="4.5.0.0" />
      </dependentAssembly>
    </assemblyBinding>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Ink" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.7.2600.2180" newVersion="1.7.2600.2180" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>