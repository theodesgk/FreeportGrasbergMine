.expr DISPTIME() (
      (DSP$TIME+DSP$START)[%h]
      )
..
.expr EQUIP() ( DSP$EQMT[%..2s] )
..
.expr OPER() (
      case( int DSP$UNIT )
      ..(
        unit_truck : T$OPNAME.4 = FMT(3) ?
                       (dictdef_nooper)[^[[5;31m%s^[[0m] :
                       T$OPNAME.4 = T$OPNAME.2 ?
                         ( T$OPNAME.4 )[^[[5;32m%s^[[0m] :
                         ( T$OPNAME.4 )[^[[5;33m%s^[[0m]
        unit_excav : E$OPNAME.4 = FMT(3) ?
                       (dictdef_nooper)[^[[5;31m%s^[[0m] :
                       E$OPNAME.4 = E$OPNAME.2 ?
                         ( E$OPNAME.4 )[^[[5;32m%s^[[0m] :
                         ( E$OPNAME.4 )[^[[5;33m%s^[[0m]
        default    : A$OPNAME.4 = FMT(3) ?
                       (dictdef_nooper)[^[[5;31m%s^[[0m] :
                       A$OPNAME.4 = A$OPNAME.2 ?
                         ( A$OPNAME.4 )[^[[5;32m%s^[[0m] :
                         ( A$OPNAME.4 )[^[[5;33m%s^[[0m]
        )
      )
..
.expr EMERG() (
      PRINTF("^G^[[1;5;31m^[#3###%{EQUIP} has an EMERGENCY.\n"),
      PRINTF("^[[1;5;31m^[#4###%{EQUIP} has an EMERGENCY.^[[0m\n"),
      PRINTF("^G^[[1;5;33m^[#3###%{EQUIP} has an EMERGENCY.\n"),
      PRINTF("^[[1;5;33m^[#4###%{EQUIP} has an EMERGENCY.^[[0m\n"),
      PRINTF("^G^[[1;5;35m^[#3###%{EQUIP} has an EMERGENCY.\n"),
      PRINTF("^[[1;5;35m^[#4###%{EQUIP} has an EMERGENCY.^[[0m\n"),
      nil
      )
