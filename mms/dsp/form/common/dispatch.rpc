0x01a8		GPS-QUERY	gpsout		d,s
0x01a9		GPS-SIMUL	gps		ddddd
0x01aa		GPS-TESTMODE	gpsout		?d
0x01ab		GPS-TEST	gps		s
log		GPS-TEST
0x01ac		GPS-DIFF	gpsdiff		d[bdbb]?b[bb]
0x01ad		GPS-PATH	gps		d[dddb]?b[ddb]
