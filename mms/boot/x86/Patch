#ifndef CONTROL
	echo you must specify a control file (and optional payload cab) to download
        echo 'example:  net/x86/Patch  CONTROL=P1 or P2 or ...'
        echo 'example:  net/x86/Patch  CONTROL=P1 CAB=yes ...'
        echo ' '
#else
        echo Downloading control file for CONTROL
	CP(x86/CONTROL/control.xml ,D:\mms\Patch\control.xml)
#if defined(CAB)
        echo Downloading cab file for CONTROL
	CP(x86/CONTROL/CONTROL.cab ,D:\mms\Patch\\CONTROL.cab)
#endif
