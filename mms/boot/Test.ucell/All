#!/psw/cpp
#include <Main/CheckRev.h>
#if #args(OK)
#	include <Main/Ram0.h>
	LD(Test,Ram0/Test)
#	ifndef MODEM
#		define MODEM ucell9600
#	endif
#	ifndef CHAN
#		define CHAN 1
#	endif
#	ifndef UP
#		define UP Loop
#	endif
	echo	Loading modems...
	CP(Dsp/MODEM.dsp,Ram0/MODEM.dsp)
	ln	Ram0/MODEM.dsp		Rp.dsp
	CP(Cfg/Rp.cfg 1 CHAN _MODEM_=MODEM,Ram0/Rp.cfg)
	CP(Test.ucell/Test.cfg _UP_=UP,Ram0/Test.cfg)
	boot	Ram0/Test lib:ram0:boot etc:ram0:boot
#endif
